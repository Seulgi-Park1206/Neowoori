<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.neowoori.app.IDaojsb">
	<select id="jsbSelectlocation" resultType="com.neowoori.app.BStudy">
		select * from study
	</select>
	<select id="jsbGetTest" resultType="com.neowoori.app.BStudy">
		select * from study
	</select>
	<insert id="jsbCreateStudy">
		insert into study
		values(study_seq.nextval,#{param1},#{param2},#{param3},#{param4},#{param5},#{param6},#{param7},#{param8},#{param9},#{param10},#{param11},#{param12},#{param13})
	</insert>
	<select id="jsbGetUser" resultType="com.neowoori.app.BMembers">
		select * from members where USERID=#{param1}
	</select>
</mapper>