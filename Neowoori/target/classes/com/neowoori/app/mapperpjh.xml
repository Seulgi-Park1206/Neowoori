<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.neowoori.app.IDaopjh">
	<insert id="pjhSignDao">
		insert into test_members1(userid,uname) values (#{param1},#{param2})
	</insert>
	
	
	<insert id="pjhsignup">
		insert into test_members(UNUM,USERID,UPW,UNAME,UNICK,UYEAR,UBIRTHDAY,UGENDER,UMOBILE,UMAIL,ADDRESS,LATI,LONGI) values (MEMBER_SEQ.nextval,#{param1},#{param2},#{param3},#{param4},#{param5},#{param6},#{param7},#{param8},#{param9},#{param10},#{param11},#{param12})
	</insert>
	
	
		
	<select id="pjhIdChk" resultType="Integer"> 
		SELECT COUNT(*) FROM test_members1 where userid = #{param1} 
	</select>
	
	
	
	
	<select id="pjhNickChk" resultType="Integer"> 
		SELECT COUNT(*) FROM test_members1 where unick = #{param1} 
	</select>
</mapper>